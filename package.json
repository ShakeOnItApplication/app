    "express-session": "^1.15.6",
    "husky": "^0.14.3",
    "jest-cli": "^22.2.2",
    "lint-staged": "^6.0.0",
    "massive": "^4.6.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "oauthio": "^0.3.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "stripe": "^5.4.0",
    "webpack": "^3.10.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
